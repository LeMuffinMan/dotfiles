{
  "disable_ai": true,
  "inlay_hints": {
    "enabled": true,
    "show_value_hints": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50,
    "toggle_on_modifiers_press": {
      "control": false,
      "alt": false,
      "shift": false,
      "platform": false,
      "function": false,
    },
  },
  "lsp": {
    "rust-analyzer": {
      "settings": {
        "rust-analyzer": {
          "rustc": {
            "source": "/home/oelleaum/.rust_src/rust-src-$VERSION/rust-src/lib/rustlib/src/rust/library",
          },
          "cargo": {
            "loadOutDirsFromCheck": true,
          },
        },
      },
    },
  },
  "icon_theme": "Zed (Default)",
  "vim_mode": true,
  "ui_font_size": 18.0,
  "buffer_font_size": 17.0,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Ayu Mirage",
  },
  "vim": {
    "leader_key": "space",
  },
}
